{
    "name": "fort/php",
    "description": "PHP Global helpers",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fort\\": "src/",
            "Fort\\PHP\\": [
                "src/PHP/",
                "src/Framework/Laravel/",
                "src/PHP/Support/",
                "src/Exception/",
                "src/Handler/"
            ]
        },

        "files": [
            "src/PHP/Standard/Standard_c.php",
            "src/Framework/Helpers/arr.php",
            "src/Framework/Helpers/math.php",
            "src/Framework/Helpers/str.php"
        ]
